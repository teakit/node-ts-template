{
  "name": "@teakit/node-ts-template",
  "version": "0.0.1",
  "description": "A node-ts-template",
  "prettier": "prettier-config-whatskit",
  "author": "Billgo <hi@billgo.me>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "directories": {
    "dist": "dist",
    "lib": "lib"
  },
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf coverage build tmp dist",
    "compile": "npm run clean && npm run build -- --dts-resolve",
    "release": "rimraf lib && tsc -p tsconfig.release.json",
    "prepublishOnly": "npm run compile && npm run release",
    "prebuild": "npm run lint",
    "build": "tsup src/index.ts --format cjs,esm",
    "build:watch": "tsup src src/index.ts --watch",
    "prettier": "prettier --config .prettierrc --write .",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "tslib": "^2.4"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31",
    "@typescript-eslint/parser": "^5.31",
    "eslint": "^8.20",
    "eslint-config-prettier": "^8.5",
    "eslint-plugin-jest": "^26.7",
    "jest": "^28.1",
    "prettier": "^2.7",
    "prettier-config-whatskit": "^0.3.0",
    "rimraf": "^3.0",
    "ts-jest": "^28.0",
    "tsutils": "^3.21",
    "tsup": "5.11.13",
    "typescript": "^4.7"
  },
  "eslintIgnore": [
    "/**/*.js",
    "node_modules/*",
    "lib/*",
    "docs/*",
    "dist/*",
    "build/*",
    "coverage/*",
    "fixtures/*",
    "umd/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "16.15.0",
    "yarn": "1.22.19"
  },
  "engines": {
    "node": ">= 16.15"
  }
}
